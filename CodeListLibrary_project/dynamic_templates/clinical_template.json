{
  "version": 1,
  "card_type": "clinical",
  "fields": {
    "phenotype_uuid": {
      "title": "Phenotype UUID",
      "active": true,
      "field_type": "string",
      "validation": {
        "mandatory": false,
        "unique": true,
        "length": [0, 250]
      },
      "desired_input": "inputbox",
      "hide_if_empty": true,
      "requires_auth": true
    },
    "type": {
      "title": "Phenotype Type",
      "active": true,
      "field_type": "enum",
      "validation": {
        "mandatory": true
      },
      "desired_input": "dropdown",
      "options": {
        "1": "Biomarker",
        "2": "Disease or syndrome",
        "3": "Drug",
        "4": "Lifestyle risk factor",
        "5": "Musculoskeletal",
        "6": "Surgical procedure"
      },
      "filterable": true
    },
    "data_sources": {
      "title": "Data Sources",
      "active": true,
      "field_type": "int_array",
      "validation": {
        "mandatory": true
      },
      "desired_input": "tagbox",
      "desired_output": "taglist",
      "source": "DataSource",
      "query": "id",
      "relative": "name",
      "filterable": true
    },
    "event_date_range": {
      "title": "Valid Event Date Range",
      "active": false,
      "field_type": "string",
      "validation": {
        "mandatory": false,
        "regex": "(?:\\d+\\/|\\d+)+-(?:\\d+\\/|\\d+)+"
      },
      "desired_input": "datepicker"
    },
    "sex": {
      "title": "Sex",
      "active": true,
      "field_type": "enum",
      "validation": {
        "mandatory": true
      },
      "options": {
        "1": "Male",
        "2": "Female",
        "3": "Male/Female"
      },
      "desired_input": "radioinput"
    },
    "agreement_date": {
      "title": "Agreement Date",
      "active": true,
      "field_type": "string",
      "validation": {
        "mandatory": false,
        "regex": "(0[1-9]|[12]\\d|30|31)\\/(0[1-9]|1[0-2])\\/(\\d{4})"
      },
      "desired_input": "datepicker",
      "hide_if_empty": true
    },
    "coding_system": {
      "title": "Coding System",
      "active": true,
      "computed": true,
      "field_type": "int_array",
      "validation": {
        "mandatory": false
      },
      "desired_output": "taglist",
      "source": "CodingSystem",
      "query": "id",
      "relative": "name",
      "filterable": true
    },
    "phenoflowid": {
      "title": "Phenoflow ID",
      "active": true,
      "field_type": "string",
      "validation": {
        "mandatory": false,
        "regex": "https:\\/\\/kclhi\\.org\\/phenoflow\\/phenotype\\/download\\/\\d+(?:\\/)?"
      },
      "desired_input": "inputbox"
    },
    "concept_information": {
      "title": "Clinical Codes",
      "active": true,
      "field_type": "concept",
      "validation": {
        "mandatory": false
      },
      "desired_input": "conceptsfield"
    },
    "source_reference": {
      "title": "Source Reference",
      "active": false,
      "field_type": "string",
      "validation": {
        "mandatory": false,
        "length": [0, 250]
      },
      "desired_input": "inputbox",
      "hide_if_empty": true
    }
  }
}
