         "phenotype_uuid": {
             "title": "Phenotype UUID",
             "field_type": "string_inputbox_code",
             "active": true,
             "requires_auth": true,
             "validation": {
                 "type": "string",
                 "mandatory": false,
                 "unique": true,
                 "length": [0, 250]
             },
             "search": {
                 "api": "true"
             },
             "hide_if_empty": true
         },
         "type": {
             "title": "Phenotype Type",
             "field_type": "enum_badge",
             "active": true,
             "validation": {
                 "type": "enum",
                 "mandatory": true,
                 "options": {
                     "1": "Biomarker",
                     "2": "Disease or syndrome",
                     "3": "Drug",
                     "4": "Lifestyle risk factor",
                     "5": "Musculoskeletal",
                     "6": "Surgical procedure"
                 }
             },
             "search": {
                 "filterable": true,
                 "api": true
             }
         },
         "data_sources": {
             "title": "Data Sources",
             "field_type": "data_sources",
             "active": true,
             "validation": {
                 "type": "int_array",
                 "mandatory": true,
                 "source": {
                     "table": "DataSource",
                     "query": "id",
                     "relative": "name"
                 }
             },
             "search": {
                 "filterable": true,
                 "api": true
             }
         },
         "event_date_range": {
             "title": "Valid Event Date Range",
             "field_type": "string_inputbox",
             "active": false,
             "validation": {
                 "type": "string",
                 "mandatory": false,
                 "regex": "(?:\\d+\\/|\\d+)+-(?:\\d+\\/|\\d+)+"
             }
         },
         "sex": {
             "title": "Sex",
             "field_type": "enum_badge",
             "active": true,
             "validation": {
                 "type": "enum",
                 "mandatory": true,
                 "options": {
                     "1": "Male",
                     "2": "Female",
                     "3": "Male/Female"
                 }
             },
             "search": {
                 "api": true
             }
         },
         "agreement_date": {
             "title": "Agreement Date",
             "field_type": "date",
             "active": true,
             "validation": {
                 "type": "string",
                 "mandatory": false,
                 "regex": "(0[1-9]|[12]\\d|30|31)\\/(0[1-9]|1[0-2])\\/(\\d{4})"
             },
             "hide_if_empty": true
         },
         "coding_system": {
             "title": "Coding System",
             "field_type": "coding_system",
             "active": true,
             "validation": {
                 "type": "int_array",
                 "mandatory": false,
                 "computed": true,
                 "source": {
                     "table": "CodingSystem",
                     "query": "id",
                     "relative": "name"
